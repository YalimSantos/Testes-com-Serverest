{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "required": [
        "nome",
        "preco",
        "descricao",
        "quantidade",
        "_id"
    ],
    "additionalProperties": true,
    "properties": {
        "nome": {
            "type": "string"
        },
        "preco": {
            "type": "integer"
        },
        "descricao": {
            "type": "string"
        },
        "quantidade": {
            "type": "integer"
        },
        "_id": {
            "type": "string"
        }
    }
}